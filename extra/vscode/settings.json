    "vscord.status.image.large.notInFile.key": "https://i.imgur.com/1kI2ro5.jpeg",
    "vscord.status.image.small.notInFile.key": "https://i.imgur.com/ZNiaJLn.jpeg",
    "vscord.status.image.small.idle.key": "https://i.imgur.com/ZNiaJLn.jpeg",
    "vscord.status.image.small.viewing.key": "https://i.imgur.com/ZNiaJLn.jpeg",
    "vscord.status.image.small.editing.key": "https://i.imgur.com/ZNiaJLn.jpeg",
    "window.commandCenter": false,
    "extensions.ignoreRecommendations": true
