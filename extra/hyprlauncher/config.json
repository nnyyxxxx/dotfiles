{
  "debug": {
    "disable_auto_focus": false,
    "enable_logging": false
  },
  "theme": {
    "colors": {
      "border": "#436096",
      "item_bg": "#0c0e18",
      "item_bg_hover": "#436096",
      "item_bg_selected": "#436096",
      "item_description": "#cee1f1",
      "item_name": "#cee1f1",
      "item_path": "#cee1f1",
      "search_bg": "#436096",
      "search_bg_focused": "#436096",
      "search_caret": "#cee1f1",
      "search_text": "#cee1f1",
      "window_bg": "#0c0e18"
    },
    "corners": {
      "list_item": 8,
      "search": 8,
      "window": 12
    },
    "spacing": {
      "item_margin": 6,
      "item_padding": 4,
      "search_margin": 12,
      "search_padding": 12
    },
    "typography": {
      "item_description_size": 12,
      "item_name_size": 14,
      "item_path_font_family": "monospace",
      "item_path_size": 12,
      "search_font_size": 16
    }
  },
  "window": {
    "anchor": "left",
    "border_width": 2,
    "custom_navigate_keys": {
      "delete_word": "h",
      "down": "j",
      "up": "k"
    },
    "height": 1430,
    "margin_bottom": 0,
    "margin_left": 0,
    "margin_right": 0,
    "margin_top": 0,
    "max_entries": 50,
    "show_border": true,
    "show_descriptions": false,
    "show_icons": true,
    "show_paths": false,
    "show_search": true,
    "use_gtk_colors": false,
    "width": 600
  }
}